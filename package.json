{
  "name": "pm4",
  "version": "0.0.1",
  "description": "",
  "main": "bin/pm4",
  "scripts": {
    "test": "ava -v",
    "prepare": "npm run build",
    "clean": "del bin dist",
    "build": "npm run clean && npm run build:source && npm run build:binaries",
    "build:source": "npm run build:cli && npm run build:god",
    "build:cli": "tsup ./src/pm4.ts --silent",
    "build:god": "tsup ./src/pm4-god.ts --silent",
    "build:binaries": "npm run build:cli-binary && npm run build:god-binary",
    "build:cli-binary": "nexe ./dist/pm4.js --output bin/pm4",
    "build:god-binary": "nexe ./dist/pm4-god.js --output bin/pm4-god"
  },
  "keywords": [
    "process",
    "manager"
  ],
  "author": "Alexis Tyler <xo@wvvw.me>",
  "license": "MIT",
  "devDependencies": {
    "@types/cli-table": "^0.3.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/debug": "^4.1.7",
    "@types/minimist": "^1.2.2",
    "@types/node-ipc": "^9.1.5",
    "@types/node-netstat": "^1.8.1",
    "@types/pidusage": "^2.0.1",
    "aggregate-error": "^4.0.0",
    "del-cli": "^4.0.1",
    "nexe": "^4.0.0-beta.18",
    "tsup": "^5.1.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "cli-table": "^0.3.6",
    "command-line-usage": "^6.1.1",
    "debug": "^4.3.2",
    "execa": "^5.1.1",
    "find-process": "^1.4.5",
    "interval-to-human": "github:OmgImAlexis/interval-to-human#main",
    "minimist": "^1.2.5",
    "node-ipc": "^10.1.0",
    "node-netstat": "^1.8.0",
    "pidusage": "^2.0.21",
    "pretty-bytes": "^5.6.0",
    "process-exists": "^4.1.0",
    "ps-list": "^7.2.0"
  }
}
